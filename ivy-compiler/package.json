{
  "name": "bitcoin-ivy",
  "version": "0.0.1",
  "description": "Compiler for Ivy, Chain's smart contract language.",
  "main": "lib/index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha src/test/*.ts --compilers ts:./node_modules/ts-node/register,tsx:./node_modules/ts-node/register",
    "compile-pegjs": "./node_modules/pegjs/bin/pegjs -o lib/parser.js src/parser.pegjs",
    "compile-tsc": "tsc",
    "build": "npm run compile-tsc && npm run compile-pegjs",
    "clean": "rm -rf lib"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.5",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "mocha": "^3.5.0",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.5.2",
    "uglifyjs-webpack-plugin": "1.0.0-beta.2",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.7.1"
  },
  "types": "./lib/index.d.ts",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "bcoin": "https://github.com/danrobinson/bcoin.git#compat",
    "level-js": "^2.2.4"
  }
}
